# A sample Gemfile
source "https://rubygems.org"

gem 'jekyll'
gem 'jekyll-assets'
gem 'rdiscount'
gem 'neat'
gem 'bourbon'
gem 'sass'
gem 'thin'

group :development do
  gem 'guard'
  gem 'guard-jekyll-plus'
  gem 'guard-livereload'
end

group :test do
  gem 'rake'
  gem 'scss_lint'
  gem 'html-proofer'
end
